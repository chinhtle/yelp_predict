<div class="yelp-intro-fitted"
	<h1>
		<div class="personality-text">
  			<font size="75" color="white">You are </font><b><font size="100"><%= @data %></font></b>
  		</div>
	</h1>
	<h2>
		<%= @description%>
	</h2>
</div>


<h2>
	<div class="panel panel-default">
		<div class="panel-heading">What variables determined your personality?</div>
	</div>
	
	<div id="variables">
		<div class="row">
  			<div class="col-xs-6 col-md-4">
  				<div class="panel panel-default">
  					<div class="variable-text">Number of Friends: </div> <%= @indep_var['Friend']%>
  				</div>
  			</div>
  			<div class="col-xs-6 col-md-4">
  				<div class="panel panel-default">
  					<div class="variable-text">Number of Reviews: </div> <%= @indep_var['Review']%>
  				</div>
  			</div>
  			<div class="col-xs-6 col-md-4">
  				<div class="panel panel-default">
  					<div class="variable-text">Number of Fans: </div> <%= @indep_var['Fan']%>
  				</div>
  			</div>
  		</div>
  		<div class="row">
  			<div class="col-xs-6 col-md-4">
  				<div class="panel panel-default">
  					<div class="variable-text">Number of times Elite: </div> <%= @indep_var['elite']%>
  				</div>
  			</div>
  			<div class="col-xs-6 col-md-4">
  				<div class="panel panel-default">
  					<div class="variable-text">Average rating given: </div> <%= @indep_var['Average']%> <i class="fa fa-star"></i>
  				</div>
  			</div>
  			<div class="col-xs-6 col-md-4">
  				<div class="panel panel-default">
  					<div class="variable-text">Number of compliments: </div> <%= @indep_var['Compliment']%>
  				</div>
  			</div>
  		</div>
  		<div class="row">
  			<div class="col-xs-12 col-md-8">
  				<div class="panel panel-default">
    				<div class="variable-text">Each type of compliment</div> 
    				<div class="variable-icon-block">
   						<div style = "display:inline-block;">
   							<i title="Like Your Profile" class="fa fa-user"></i> 
   							<div>
   								<%= @indep_var['profile']%>
   							</div>
   						</div>
   						<div style = "display:inline-block;">
   							<i title="Cute Pic" class="fa fa-heart"></i> 
   							<div>
   								<%= @indep_var['cute']%>
   							</div>
   						</div>
   						<div style = "display:inline-block;">
   							<i title="You're Funny" class="fa fa-smile-o"></i> 
   							<div>
   								<%= @indep_var['funny']%>
   						</div>
   						</div>
   						<div style = "display:inline-block;">
   							<i title="Thank you" class="fa fa-gift"></i> 
   							<div>
   								<%= @indep_var['plain']%>
   						</div>
   						</div>
   						<div style = "display:inline-block;">
   							<i title="Good Writer" class="fa fa-pencil"></i> 
   							<div>
   								<%= @indep_var['writer']%>
   							</div>
   						</div>
   						<div style = "display:inline-block;">
   							<i title="Great Lists" class="fa fa-list"></i> 
   							<div>
   								<%= @indep_var['list']%>
   							</div>
   						</div>
   						<div style = "display:inline-block;">
   							<i title="Just a Note" class="fa fa-file-o"></i> 
   							<div>
   								<%= @indep_var['note']%>
   							</div>
   						</div>
   						<div style = "display:inline-block;">
   							<i title="Great Photo" class="fa fa-picture-o"></i> 
   							<div>
   								<%= @indep_var['photos']%>
   							</div>
   						</div>
   						<div style = "display:inline-block;">
   							<i title="Hot Stuff" class="glyphicon glyphicon-fire"></i> 
   							<div>
   							<%= @indep_var['hot']%>
   						</div>
   						</div>
   						<div style = "display:inline-block;">
   							<i title="Write More" class="fa fa-plus"></i> 
   							<div>
   								<%= @indep_var['more']%>
   							</div>
   						</div>
   						<div style = "display:inline-block;">
   							<i title="You're Cool" class="fa fa-cloud"></i> 
   							<div>
   								<%= @indep_var['cool']%>
   							</div>
   						</div>
					</div>
			</div>
		</div>
   			<div class="col-xs-6 col-md-4">
   				<div class="panel panel-default">
   					<div class="variable-text">Each type of vote</div>
   					<div class="variable-icon-block">
   						<div style = "display:inline-block;">
   							<i title="Useful" class="fa fa-thumbs-up"></i> 
   							<div>
   								<%= @indep_var['Useful']%>
   							</div>
   						</div>
   						<div style = "display:inline-block;">
   							<i title="Funny" class="fa fa-smile-o"></i> 
   							<div>
   								<%= @indep_var['Funny']%>
   							</div>
   						</div>
   						<div style = "display:inline-block;">
   							<i title="Cool" class="fa fa-cloud"></i> 
   							<div>
   								<%= @indep_var['Cool']%>
   							</div>
   						</div>
   					</div>
   				</div>
   			</div>
		</div>
	</div>
</h2>

